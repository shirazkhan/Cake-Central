ype: 'TOGGLE_CART_MENU'})} passHref href = {`/shop/${f.productType}/${f.productHandle}#${f.variantHandle}`}>\n                <div>\n                    <Image sizes=\"50vw\" objectFit = {'contain'} layout = {'responsive'} height = {'100%'} width = {'100%'} src = {f.imgSrc} />\n                    <ProductSpec>\n                        <Title>{f.productTitle}</Title>\n                        <SubTitle >{f.variantTitle}</SubTitle>\n                        <Price>£{f.price}</Price>\n                        <AddRemoveContainer>\n                            <span>Add To Bag</span>\n                            <a onClick = {() => dispatch({type:'REMOVE_FROM_WISHLIST', value: wishList.filter(ff => f.variantId !== ff.variantId)})}>Remove</a>\n                        </AddRemoveContainer>\n                    </ProductSpec>\n                </div>\n            </Link>\n        </ProductCard>\n    );\n\nexport default function WishList({isWishList}) {\n\n    const { globalState, dispatch } = useContext(GlobalStateContext);\n\n    const { ref, inView } = useInView({threshold: 0.2});\n\n    useEffect(() => {\n        if(inView === false){\n            dispatch({type: 'SET_WISHLIST_FALSE'})\n        }\n    },[inView])\n\n    return (\n        <ScrollIntoViewIfNeeded active = {isWishList}>\n            {\n                globalState.wishList.length\n                ? <WishListContainer ref = {ref} id = 'wishlist'>\n                        {renderFavouriteCards(globalState.wishList, dispatch)}\n                  </WishListContainer>\n                : <div>There's nothing in your favourites...</div>\n            }\n        </ScrollIntoViewIfNeeded>\n    )\n}\n","/Users/shirazkhan/Documents/Projects/NextJS-Shopify-Wordpress-Starter/components/productCarousel/Indicator.js",[],"/Users/shirazkhan/Documents/Projects/NextJS-Shopify-Wordpress-Starter/components/productCarousel/ProductImages.js",["283","284"],"import React, {useState, useRef, useEffect} from 'react';\nimport Image from '