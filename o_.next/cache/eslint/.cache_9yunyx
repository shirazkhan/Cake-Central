 {i} active = {inView}>\n                            <Image\n                                placeholder = 'blur'\n                                blurDataURL={`data:image/svg+xml;base64,${toBase64(shimmer(700, 475))}`}\n                                layout = 'fill'\n                                src = {img.src}\n                            />\n                        </ImageContainer>\n                    )}\n                </InView>\n        }\n    )\n}\n\nexport default function ProductImages({images, variants}) {\n\n    const [imageIdx, setImageIdx] = useState(0);\n    const myRef = useRef();\n    return (\n        <>\n            <Container vertical = {false} className = 'scroll-container'>\n                {renderImages(images, variants, setImageIdx,myRef)}\n            </Container>\n            <Indicator quantity = {images.length} idx = {imageIdx} />\n        </>\n    )\n}\n",{"ruleId":"285","severity":1,"message":"286","line":6,"column":54,"nodeType":"287","messageId":"288","endLine":6,"endColumn":63},{"ruleId":"285","severity":1,"message":"289","line":18,"column":7,"nodeType":"287","messageId":"288","endLine":18,"endColumn":28},{"ruleId":"290","severity":1,"message":"291","line":71,"column":20,"nodeType":"292","messageId":"293","endLine":71,"endColumn":21,"fix":"294"},{"ruleId":"290","severity":1,"message":"291","line":73,"column":20,"nodeType":"292","messageId":"293","endLine":73,"endColumn":21,"fix":"295"},{"ruleId":"290","severity":1,"message":"291","line":75,"column":20,"nodeType":"292","messageId":"293","endLine":75,"endColumn":21,"fix":"296"},{"ruleId":"290","severity":1,"message":"291","line":77,"column":20,"nodeType":"292","messageId":"293","endLine":77,"endColumn":21,"fix":"297"},{"ruleId":"298","severity":1,"message":"299","line":179,"column":5,"nodeType":"300","endLine":179,"endColumn":7,"fix":"301"},{"ruleId":"302","severity":2,"message":"303","line":6,"column":7,"nodeType":"304","endLine":6,"endColumn":13},{"ruleId":"302","severity":2,"me